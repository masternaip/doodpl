        }
        .gallery-link {
            display: block;
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0; left: 0;
            z-index: 2;
            text-indent: -9999px;
        }
        .gallery-item:hover {
            filter: brightness(0.92);
        }
        /* MOBILE RESPONSIVE ENHANCEMENTS */
        @media (max-width: 900px) {
            .gallery-container {
                grid-template-columns: repeat(2, 1fr);
